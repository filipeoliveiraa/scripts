class UglyCipherTester:
        def input_by_string(self):
                uc   = UglyCipher()
                word = "this is test words."
                key    = 5
                c      = 's'
                a      = 4
                actual = uc.buildCipher(word,key,c,a)
                # assert actual == expect
                return actual

